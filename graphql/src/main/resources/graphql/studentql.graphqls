type Student {
    id: UUID!
    name: String!
    address: String
    dateOfBirth: String
    createdAt: DateTime!
    updatedAt: DateTime!
}

extend type Query {
    students(limit: Int):[Student]
    student(id: UUID):Student
}

extend type Mutation {
    createStudent(input: CreateStudentInput!):Student!
    createStudents(inputs: [CreateStudentInput!]!):[Student!]!
}

input CreateStudentInput {
    name: String!
    address: String
    dateOfBirth: String
}
